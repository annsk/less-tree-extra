<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>{{title}}</title>
    <link href="./index.css" media="all" rel="stylesheet">
</head>
<body class="today-log">
    <a class="back-to-list" href="./list.html">回到发布列表</a>
    <h1>发布时间: {{time}}</h1>
    <div class="group">
        <h2>影响页面</h2>
        <ul class="relative-pages">
	    {{#each (pages)}}
            <li><a href="{{this.url}}">{{this.name}}</a></li>
        {{/each}}
        </ul>
    </div>
    <div class="group">
        <h2>改动文件</h2>
        <ul class="changed-src-files">
	    {{#each (diffFiles)}}
            <li>{{this}}</li>
        {{/each}}
        </ul>
    </div>
    <div class="group">
        <h2>打包文件</h2>
        <ul class="changed-release-files">
	    {{#each (assets)}}
            <li>{{this.name}}</li>
        {{/each}}
        </ul>
    </div>
</body>
</html>

