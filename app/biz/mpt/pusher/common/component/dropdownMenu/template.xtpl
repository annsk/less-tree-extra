<div class="sort-detail">
    {{#each(items, index)}}
        <div
            {{#if( xindex===0 )}}
                class="item current J_log"
            {{else}}
                class="item J_log"
            {{/if}}

            data-sort="{{this.key}}"

            data-json__block_name="贝贝赚宝_开始助力_分类tab排序项"
            data-json__tab="{{this.name}}"
            data-json__biz="zhuanbao"
        >
            {{this.name}}  <i class="ico-tick"></i>
        </div>
    {{/each}}
</div>
