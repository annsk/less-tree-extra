{{#each (data.rank_groups)}}
	<li class="item{{#if (this.current)}} current{{/if}}">
		<i class="top{{this.key}}">
			{{#if (this.key > 3)}}
				{{this.key}}
			{{/if}}
		</i>
		<span>{{this.group_nick}}</span>
		<span>{{this.group_score}}人参加</span>
	</li>
{{/each}}
{{#if (!data.isEnd)}}
	<div class="more J_more">
		<span>查看更多</span>
		<i class="icon-down"></i>
	</div>
{{/if}}
