<div class="cats" id="J_cats">
    <p>类别</p>
    <span class="option J_option-all" data-cid='0'>全部</span>
    {{#each (data.cates)}}
        <span class="option J_option" data-cid={{this.cid}}>{{this.cate_name}}</span>
    {{/each}}
</div>
<div class="brands" id="J_brands">
    <p>品牌</p>
    <span class="option J_option-all" data-bid='0'>全部</span>
    {{#each (data.brands)}}
        <span class="option J_option" data-bid={{this.brand_id}}>{{this.brand_name}}</span>
    {{/each}}
</div>