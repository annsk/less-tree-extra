<div id="J_mores" class="mores">
    <div id="J_mores_list" class="mores-list">
        {{#if (data.fightgroup_items.length) }}
            <ul data-cat="mores"></ul>
        {{/if}}
        {{#each (list,"value","key") }}
            <ul class="{{#if (data.fightgroup_items.length || xindex !== 0) }}hidden{{/if}}" data-cat={{key}}></ul>
        {{/each}}
    </div>
</div>